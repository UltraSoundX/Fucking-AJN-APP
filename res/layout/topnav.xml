<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
    <View android:id="@+id/topblank" android:background="@mipmap/topnav_bg" android:layout_width="match_parent" android:layout_height="20dp"/>
    <RelativeLayout android:background="@mipmap/topnav_bg" android:layout_width="match_parent" android:layout_height="48dp">
        <ImageView android:id="@+id/backbutton" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginLeft="5dp" android:src="@mipmap/back" android:scaleType="center" android:layout_alignParentLeft="true" android:layout_centerVertical="true" android:contentDescription="@null"/>
        <ImageView android:id="@+id/minsheng" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="36dp" android:layout_marginLeft="10dp" android:src="@mipmap/left_minsheng" android:scaleType="center" android:layout_alignParentLeft="true" android:layout_centerVertical="true" android:contentDescription="@null"/>
        <TextView android:textSize="22sp" android:textColor="@color/white" android:id="@+id/topbartxt" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerHorizontal="true" android:layout_centerVertical="true"/>
        <ImageView android:id="@+id/ajnlogo" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="36dp" android:src="@mipmap/ajnlogo" android:layout_centerInParent="true" android:layout_centerVertical="true" android:contentDescription="@null"/>
        <TextView android:textSize="16sp" android:textColor="@color/white" android:gravity="center" android:id="@+id/wangqiliulan" android:background="@drawable/btn_topnavright" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="32dp" android:layout_marginRight="5dp" android:text="@string/wangqiliulan" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
        <TextView android:textSize="18sp" android:textColor="@color/white" android:id="@+id/daka" android:background="@drawable/btn_topnavright" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="5dp" android:text="@string/daka" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
        <ImageView android:id="@+id/sousuo" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="36dp" android:layout_marginLeft="10dp" android:layout_marginRight="10dp" android:src="@mipmap/right_search" android:scaleType="center" android:layout_alignParentRight="true" android:layout_centerVertical="true" android:contentDescription="@null"/>
        <ImageView android:id="@+id/share" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="10dp" android:src="@mipmap/fenxiangimg" android:layout_alignParentRight="true" android:layout_centerVertical="true" android:contentDescription="@null"/>
    </RelativeLayout>
</LinearLayout>
